{
  "name": "vue2-goto-definition",
  "displayName": "vue2-goto-definition",
  "description": "Simple go to component definition",
  "version": "0.0.3",
  "publisher": "jmunoz",
  "author": {
    "name": "jmu√±oz",
    "email": "julianmc90@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuusuario/vue2-goto-definition.git"
  },
  "bugs": {
    "url": "https://github.com/tuusuario/vue2-goto-definition/issues"
  },
  "homepage": "https://github.com/tuusuario/vue2-goto-definition",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vue",
    "goto",
    "definition",
    "vue2",
    "hover"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onLanguage:vue"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vue2-goto-definition.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.2",
    "eslint": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2"
  }
}
